{
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "files.trimTrailingWhitespace": true,
  "prettier.disableLanguages": [
    "javascript",
    "javascriptreact",
    "typescript",
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "vue",
  ],
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}
